{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "preLaunchTask": "tsc-watch",
      "name": "Attach to Docker 9229",
      "port": 9229,
      "address": "localhost",
      "protocol": "inspector",
      "restart": true,
      "localRoot": "${workspaceFolder}/server/dist",
      "remoteRoot": "/server/dist",
      "outFiles": [
        "${workspaceFolder}/server/dist/**/*.js"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js",
      ]
    }
  ]
}